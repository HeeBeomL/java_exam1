package LHB.S1;

import java.util.ArrayList;
import java.util.List;

class Stirng2{
    String a;
    String b;
    String c;


    Stirng2(String a, String b, String c ){
        this.a = a;
        this.b = b;
        this.c = c;
    }

}
public class Stirng1 {
    public static void main(String[] args) {
        Stirng2[] s2 = new Stirng2[3];
        Stirng2 s = new Stirng2("이희범", "고승준", "조원장");
        s2[0] = s;
        System.out.println(s2[0].a);

        List<String> list = new ArrayList<>();
        System.out.println(list.size());





    }
}
